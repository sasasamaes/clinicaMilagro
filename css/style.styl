@import "normalize.styl"
body
	font-family sans-serif
	font-size 16px
	width 100%
header
	color #C1B009
	height 455px
.header__Index
	background-image: url("../img/Inicio/unoMovil.jpg");
	background-position: cover;
.titulo
	font-size 1.5rem
	text-align center
	padding .6rem
.headerQuienesSomos
	background-image: url("../img/QuienesSomos/headerQuienesSomos.jpg");
	background-position: cover;
.headerTratamientos
	background-image: url("../img/Tratamentos/movilUno.jpg");
	background-position: cover;
header .logo
	height 83px
	background-color #120B0B
	margin-top 2em
header nav a
	display: block
	text-align: center
	text-decoration: none
	color:white
	margin-bottom: .8rem
	padding-top: .5rem
main .bloqueUno
	height: 337px
	background-color: #9E9E9E
	text-align: center
	padding-top:16em
	color:white
	font-size: .8rem
main .bloqueUno h2
	width: 100%
	margin: 0 auto
	margin-bottom: 1rem
main .bloqueUno p
	width: 100%
	margin: 0 auto
main .bloqueDos
	background-color: #F5F5F5
	text-align center
main .bloqueDos p,main .bloqueDos ul
	width: 100%
	margin: 0 auto
	margin-top: 6.9rem
	margin-bottom: 6.9rem
	font-size: .8rem
main .inicio::before
	content: ""
	width: 100%
	height: 200px
	background-image: url(../img/Inicio/dosMovil.jpg);
	display: block
main .inicio::after
	content: ""
	width: 100%
	height: 170px
	background-image: url(../img/Inicio/tresMovil.jpg);
	display: block
main .quienesSomos::before
	content: ""
	width: 100%
	height: 250px
	background-image: url(../img/QuienesSomos/dosMovil.jpg);
	display: block
main .quienesSomos::after
	content: ""
	width: 100%
	height: 300px
	background-image: url(../img/QuienesSomos/tresMovil.jpg);
	display: block
main .tratamientos::before
	content: ""
	width: 100%
	height: 180px
	background-image: url(../img/Tratamentos/movilTres.jpg);
	background-position: center
	display: block
main .tratamientos::after
	content: ""
	width: 100%
	height: 250px
	background-image: url(../img/Tratamentos/movilDos.jpg);
	display: block
footer
	height: 50px
footer p
	font-size .5rem
.left
	float left
.right
	float right
.clear
	clear both
	overflow hidden

